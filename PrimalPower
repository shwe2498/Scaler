/*
 Primal Power

 "Primal Power" of an array is defined as the count of prime numbers present in it.
  Given an array of integers A of length N, you have to calculate its Primal Power.
 
 Input 1: A = [-6, 10, 12] Output 1: 0
 Input 2: A = [-11, 7, 8, 9, 10, 11] Output 2: 2
 */

#include <iostream>
#include <vector>
#include <math.h>

using namespace std;

int solve(vector<int> &A) {
    int primalPower = 0;
    
    for(int i = 0; i < A.size(); i++){
        bool isPrime = true;
        if(A[i] <= 1){
            isPrime = false;
        }
        else
        {
            for(int j = 2; j <= sqrt(A[i]); j++){
                if(A[i] % j == 0){
                    isPrime = false;
                    break;
                }
            }
        
            if(isPrime){
                primalPower++;
            }
        }
    }
    return primalPower;
}
